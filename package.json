  "scripts": {
    "start": "rm -rf deploy/build deploy/build.zip && forever start deploy/productionServer.js && webpack-dev-server",
    "stop": "kill -9 $(pgrep node) && kill -9 $(pgrep -f webpack) && forever stop deploy/productionServer.js",
    "pro": "rm -rf deploy/build deploy/build.zip && echo 'Processing application files...' && NODE_ENV=production webpack -p  > /dev/null && echo 'Compiling application files...' && NODE_ENV=production webpack -p --progress && zip -r deploy/build.zip deploy/build"
  },
